{
  "name": "s2s",
  "version": "0.8.0",
  "description": "Source To Source",
  "license": "MIT",
  "repository": "https://github.com/akameco/s2s",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "s2s": "./bin/s2s.js"
  },
  "main": "./dest/index.js",
  "scripts": {
    "add": "all-contributors add",
    "prebuild": "rimraf dest",
    "build": "babel src -d dest --ignore *.test.js,__tests__",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint src/",
    "precommit": "lint-staged",
    "postinstall": "npm run fmt",
    "prepare": "npm run build",
    "fmt": "prettier --write '**/*.{js,json}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci":
      "jest --coverage --ci --runInBand && cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "*.js": ["fmt", "lint", "test", "git add"],
    "*.json": ["fmt", "git add"]
  },
  "files": ["dest", "bin"],
  "keywords": ["s2s"],
  "dependencies": {
    "babel-core": "^7.0.0-beta.2",
    "babel-polyfill": "^7.0.0-beta.2",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "cp-file": "^5.0.0",
    "find-up": "^2.1.0",
    "prettier": "^1.7.4",
    "slash": "^1.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.5.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.3.0-beta.1",
    "babel-plugin-transform-flow-strip-types": "^7.0.0-beta.2",
    "babel-preset-env": "^2.0.0-beta.2",
    "babel-preset-stage-2": "^7.0.0-beta.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-precure": "^0.8.1",
    "flow-bin": "^0.56.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "path-exists": "^3.0.0",
    "pretty-format": "^21.2.1",
    "rimraf": "^2.6.2",
    "strip-ansi": "^4.0.0"
  },
  "peerDependencies": {}
}
